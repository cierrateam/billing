{
    "name": "cierrateam/billing",
    "description": "Billing - Package for Laravel",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.*"
    },
    "author": {
        "name": "Vittorio Emmermann ",
        "email": "vittorio@cierra.eu"
    },
    "autoload": {
        "psr-4": {
            "Cierrateam\\Billing\\": "src/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Cierrateam\\Billing\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cierrateam\\Billing\\BillingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0",
        "laravel/framework": "~5.4.36|~5.5.0|~5.6.0",
        "orchestra/testbench": "~3.4.0|~3.5.0|~3.6.0",
        "orchestra/database": "~3.4.0|~3.5.0|~3.6.0",
        "orchestra/testbench-core": "~3.4.0|~3.5.0|~3.6.0",
        "laravel/browser-kit-testing": "~1.0.0|~2.0.0",
        "orchestra/testbench-browser-kit": "~3.4.0|~3.5.0|~3.6.0"
    }
}
